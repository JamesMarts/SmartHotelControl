<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suozhang.smarthome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TVControlActivity" />
        <activity android:name=".activity.AirConditionControlActivity" />
        <activity android:name=".activity.LigthControlActivity" />
        <activity android:name=".activity.ServiceControlActivity" />
        <activity android:name=".activity.ModelControlActivity" />
        <activity android:name=".activity.OtherControlActivity"></activity>
    </application>

</manifest>